import java.io.*;
import java.util.*;

public class Solution {

   public void sum(int a,int b)
    {
        int c=a+b;
      System.out.print(c);
        
       
    }

public static void main(String[] args){
    
    Scanner s=new Scanner(System.in);
    Solution ob=new Solution();
    int a=s.nextInt();
    int b=s.nextInt();
    ob.sum(a,b);
    
}}
