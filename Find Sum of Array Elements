import java.io.*;
import java.util.*;

public class Solution {
         public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int N=s.nextInt();
        int sum=0;    
        for(int i=1;i<=N;i++)
        {
           int num=s.nextInt();
            sum+=num;
        }
            System.out.print(sum);
        }
    }
   
