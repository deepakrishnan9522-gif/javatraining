import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner s=new Scanner(System.in);
        int a,b,c;
        a=s.nextInt();
        b=s.nextInt();
        c=s.nextInt(); 
        
        int largest =a;
         
        if(b>largest){
           largest=b;
        }
        
        if(c>largest){
           largest=c;
        }
        
        System.out.println(largest);
            
    }
}
