import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String a=s.nextLine();
        int i=s.nextInt();
        int j=s.nextInt();
        StringBuilder b=new StringBuilder(a);
        b.delete(i,j);
        System.out.println(b);
    }
}
