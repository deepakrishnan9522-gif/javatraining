import java.util.Scanner;

public class RemoveSpaces {
    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);

        
        String inputS = sc.nextLine();

       
        String result = inputS.replaceAll("\\s", "");

        
        System.out.println(result);

        
        sc.close();
    }
}
